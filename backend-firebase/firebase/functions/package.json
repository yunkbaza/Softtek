{
  "name": "softtek-sprint2-firebase",
  "version": "1.0.0",
  "private": true,
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "serve": "npm run build && firebase emulators:start --import=./_data --export-on-exit",
    "deploy": "npm run build && firebase deploy --only functions,firestore:rules,firestore:indexes"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "firebase-admin": "^12.5.0",
    "firebase-functions": "^5.0.1",
    "express-rate-limit": "^7.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "@types/express": "^4.17.21"
  }
}